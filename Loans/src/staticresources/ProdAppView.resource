vecancel=\"false\" searchmethod=\"server\" searchbox=\"true\" showexportbuttons=\"false\" pagesize=\"10\" createrecords=\"false\" model=\"Ledger\" buttonposition=\"\" mode=\"readonly\"><fields><field id=\"Name\"/><field id=\"Description__c\"/><field id=\"Amount__c\"/><field id=\"Type__c\"/><field id=\"Transaction_Date_Time__c\"/></fields><rowactions/><massactions usefirstitemasdefault=\"true\"/><views><view type=\"standard\"/></views><searchfields/></skootable></components></tab></tabs></tabset></components><resources><labels/><javascript><jsitem location=\"inlinesnippet\" name=\"ViewDetails\" cachelocation=\"false\">var PropertyModel = skuid.model.getModel('SecurityProperty');\n\nvar propertyIdCondition = PropertyModel.getConditionByName('Property__c');\n\nPropertyModel.setCondition(propertyIdCondition,args.item.row.Id,true);\n\nskuid.model.updateData([PropertyModel]);</jsitem><jsitem location=\"inlinesnippet\" name=\"GetOutstanding\" cachelocation=\"false\">\n//Get the Quote Id of the row\nvar QuoteId = arguments[0].item.row.Id;\n\n\ntry{\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"GetOutstandingItems\",\"getOutstandingItems\",{QuoteId:QuoteId})); \n\nif(sResponse == \"complete\"){\nalert('Outstanding Items have been created')\n\n//Refresh Updated Models\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\nvar ApplicationModel = skuid.model.getModel('Application');\n\nOutstandingItemModel.updateData();\nApplicationModel.updateData();\nOutstandingItemNotReceivedModel.updateData();\n\n}\nelse{\nalert('There has been an error')\n}\n}\ncatch(err){\nalert(err);\n}</jsitem><jsitem location=\"external\" name=\"apex\" cachelocation=\"false\" url=\"/soap/ajax/29.0/apex.js\">var params = arguments[0],\n\t$ = skuid.$;\n</jsitem><jsitem location=\"inlinesnippet\" name=\"ConfirmPackReceived\" cachelocation=\"false\" url=\"\">var params = arguments[0],\n    $ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar CHECKBOX_FIELD = \"Received__c\";\nvar idsOfRowsThatAreChecked = [];\n\n//Get Model\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\n\n    \n//Loop through all rows and grab the Id's of the ones checked as Received\n$.each(OutstandingItemModel.data,function(i,row){\n   if (OutstandingItemModel.getFieldValue(row,CHECKBOX_FIELD)===true){\n        idsOfRowsThatAreChecked.push(row.Id);\n   }\n});\n\nif (idsOfRowsThatAreChecked.length &gt; 0){\ntry{\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"AppSummary\",\"packReceivedButton\",{OutstandingItems:idsOfRowsThatAreChecked})); \n\nif(sResponse == \"complete\"){\n\nalert('Pack Received Confirmation')\n\n//Refresh Updated Models\nvar ApplicationModel = skuid.model.getModel('Application');\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\nskuid.model.cancel([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\nskuid.model.updateData([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\n\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\nelse{\nalert('There has been an error')\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\n}\ncatch(err){\nalert(err);\n}\n}\nelse{\nalert('Please select at least one Outstanding Item')\n\n}\n\n\n</jsitem><jsitem location=\"inlinesnippet\" name=\"ConfirmSentToBank\" cachelocation=\"false\" url=\"\">var params = arguments[0],\n    $ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar ApplicationModel = skuid.model.getModel('Application');\nvar appId = ApplicationModel.data[0].Id;\n\nvar CHECKBOX_FIELD = \"Received__c\";\nvar idsOfRowsThatAreChecked = [];\n\n//Get Model\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\n\nvar allItemsChecked = true;    \n//Loop through all rows and grab the Id's of the ones checked as Received\n$.each(OutstandingItemNotReceivedModel.data,function(i,row){\n   if (OutstandingItemNotReceivedModel.getFieldValue(row,CHECKBOX_FIELD)===false){\n        allItemsChecked = false;\n   }\n   else if (OutstandingItemNotReceivedModel.getFieldValue(row,CHECKBOX_FIELD)===true){\n        idsOfRowsThatAreChecked.push(row.Id);\n   }\n});\n\nif (allItemsChecked == true){\ntry{\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"AppSummary\",\"sentToBankButton\",{applicationId:appId, OutstandingItems:idsOfRowsThatAreChecked})); \n\nif(sResponse == \"complete\"){\n\nalert('Sent to Bank Confirmation')\n\n//Refresh Updated Models\nvar ApplicationModel = skuid.model.getModel('Application');\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\n\nskuid.model.cancel([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\nskuid.model.updateData([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\n\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\nelse{\nalert('There has been an error')\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\n}\ncatch(err){\nalert(err);\n}\n}\nelse{\nalert('All Outstanding Items need to be checked to Send to Bank')\n\n}\n\n\n</jsitem><jsitem location=\"inlinesnippet\" name=\"SyncModels\" cachelocation=\"false\">\nskuid.$(function(){\n\nvar ChildModel = skuid.model.getModel('OutstandingItemNotReceived'),\nParentModel = skuid.model.getModel('OutstandingItem');\n\nvar ChildModel2 = skuid.model.getModel('OutstandingItem'),\nParentModel2 = skuid.model.getModel('OutstandingItemNotReceived');\n\nvar listener = new skuid.ui.Editor();\nlistener.handleSave = function() {\nif (!ParentModel.hasChanged) {\nParentModel.updateData();\n}\nif (!ParentModel2.hasChanged) {\nParentModel2.updateData();\n}\n};\nlistener.registerModel(ChildModel);\nlistener.registerModel(ChildModel2);\n}); </jsitem><jsitem location=\"inlinesnippet\" name=\"PaidOut\" cachelocation=\"false\">var params = arguments[0],\n    $ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar CHECKBOX_FIELD = \"Received__c\";\nvar idsOfRowsThatAreChecked = [];\n\n//Get Model\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\n\nvar paidOut = true;    \n//Loop through all rows and grab the Id's of the ones checked as Received\n$.each(OutstandingItemModel.data,function(i,row){\n   if (OutstandingItemModel.getFieldValue(row,CHECKBOX_FIELD)===false){\n        paidOut = false;\n   }\n   else{\n       idsOfRowsThatAreChecked.push(row.Id);\n   }\n});\n\nif (paidOut == true &amp;&amp; idsOfRowsThatAreChecked.length &gt; 0){\ntry{\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"AppSummary\",\"paidOut\",{OutstandingItems:idsOfRowsThatAreChecked})); \n\nif(sResponse == \"complete\"){\n\nalert('Paid Out Confirmation')\n\n//Refresh Updated Models\nvar ApplicationModel = skuid.model.getModel('Application');\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\nskuid.model.cancel([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\nskuid.model.updateData([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\n\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\nelse{\nalert('There has been an error')\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\n}\ncatch(err){\nalert(err);\n}\n}\nelse{\nalert('Sent to Bank needs to be completed first')\n\n}\n\n\n</jsitem><jsitem location=\"inlinesnippet\" name=\"GetOutsandingRequestType\" cachelocation=\"false\">//Declare Variables\nvar ApplicationModel = skuid.model.getModel('Application');\nvar ApplicationId = ApplicationModel.data[0].Id;\n\nvar AllMortgageModel = skuid.model.getModel('AllMortgage');\nvar MortgageId = AllMortgageModel.data[0].Id;\n\nvar RequestType = AllMortgageModel.data[0].Request_Type__c;\n\n\nif (!RequestType){\n    alert('You must select a Request Type');\n}\nelse{\n    try{\n    //Call the Apex Webservice Method\n    var sResponse = new String(sforce.apex.execute(\"GetOutstandingItems\",\"getOutstandingItemRequestType\",{MortgageId:MortgageId,ApplicationId:ApplicationId,RequestType:RequestType})); \n    \n    if(sResponse == \"complete\"){\n    //alert('Outstanding Items have been created')\n\n    }\n    else{\n    alert('There has been an error')\n    }\n    }\n    catch(err){\n    alert(err);\n    }\n    //Close the Popup\n    //skuid.$('.ui-dialog-content').dialog('close');\n    \n    //Refresh Updated Models\n    var OutstandingItemModel = skuid.model.getModel('OutstandingItem');\n    var OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\n    var ApplicationModel = skuid.model.getModel('Application');\n    var AllMortgageModel = skuid.model.getModel('AllMortgage');\n    var LedgerModel = skuid.model.getModel('Ledger');\n    skuid.model.cancel([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel,AllMortgageModel,LedgerModel]);\n    skuid.model.updateData([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel,AllMortgageModel,LedgerModel]);\n}\n</jsitem><jsitem location=\"inlinesnippet\" name=\"createValuationLedger\" cachelocation=\"false\">//AllProperty Model needs to be called\nvar SecurityPropertyModel = skuid.model.getModel('SecurityProperty');\n//This gets the Id of the Property\nvar row_id = SecurityPropertyModel.data[0].Id;\n//Declare vairable array to hold Ids\nvar rowids = [];\nrowids.push(row_id);\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"AppSummary\",\"payValuationFee\",{propertyIds :rowids})); \n\nif(sResponse == \"complete\"){\n\nalert('Ledger updated.')\n}\n\nif(sResponse == \"noplan\"){\n\nalert('No such Valuation plan with selected Valuer.')\n}\n\nif(sResponse == \"error\"){\n\nalert('Error - not able to update Application and Ledger.')\n}\n</jsitem></javascript><css/></resources></skuidpage>","skuid__Layout2__c":null,"skuid__Layout3__c":null,"skuid__Layout4__c":null,"skuid__Layout5__c":null}]