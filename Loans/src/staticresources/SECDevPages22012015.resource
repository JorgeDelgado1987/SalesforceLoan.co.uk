   alert('Applicants Mobile Number is invalid')\n            }\n            else{\n                alert('There has been an error')\n            }\n        }\n        catch(err){\n            alert(err);\n        }\n    }\n    else{\n        alert('There has been an error')\n    }\n}\ncatch(err){\n    alert(err);\n}\n//Close the Popup\n//skuid.$('.ui-dialog-content').dialog('close');\n\n//Refresh Updated Models\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\nvar ApplicationModel = skuid.model.getModel('Application');\nvar AllMortgageModel = skuid.model.getModel('AllMortgage');\nvar LedgerModel = skuid.model.getModel('Ledger');\nskuid.model.cancel([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel,AllMortgageModel,LedgerModel]);\nskuid.model.updateData([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel,AllMortgageModel,LedgerModel]);\n\n</jsitem><jsitem location=\"inlinesnippet\" name=\"getLenderId\" cachelocation=\"false\">var params = arguments[0],\n    step = params.step,\n$ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar CHECKBOX_FIELD = \"Select_Item__c\";\n\nvar idsOfRowsThatAreChecked = [];\nvar nameField = [];\n\n\n//Get Model\nvar QuoteWizardStep1Model = skuid.model.getModel('QuoteWizardStep1');\n\n    \n//Loop through all rows and grab the Id's of the ones checked as Received\n$.each(QuoteWizardStep1Model.data,function(i,row){\n   if (QuoteWizardStep1Model.getFieldValue(row,CHECKBOX_FIELD)===true){\n        idsOfRowsThatAreChecked.push(row.Id);\n        nameField.push(row.Name);\n   }\n});\n\nif (idsOfRowsThatAreChecked.length &gt; 1){\n    alert('You can only Select one Lender');\n}\nif (idsOfRowsThatAreChecked.length === 0){\n    alert('You must Select at least one Lender');\n}\nelse{\n    //get models and conditions\n    var QuoteWizardStep2Model = skuid.model.getModel('QuoteWizardStep2');\n    var step2LenderIdCondition = QuoteWizardStep2Model.getConditionByName('Step2LenderId');\n    \n    //Set the QuoteWizardStep2 model's condition to contain the idsOfRowsThatAreChecked, and refresh the model\n    QuoteWizardStep2Model.setCondition(step2LenderIdCondition, idsOfRowsThatAreChecked[0], true);\n    skuid.model.updateData([QuoteWizardStep2Model]);\n    \n    //Update the New Quote\n    QuoteWizardStep3Model = skuid.model.getModel('QuoteWizardStep3'),\n    QuoteWizardStep3Row = QuoteWizardStep3Model.getFirstRow();\n    var accountName = 'adfasdfasdf';\n    QuoteWizardStep3Model.updateRow(QuoteWizardStep3Row,'Lender_Fee__c',idsOfRowsThatAreChecked[0]);\n    // Make sure that the Account Field Editor is updated\n    $.each(QuoteWizardStep3Model.registeredItems,function(){\n        this.refreshFields();\n    });\n    \n    \n    //Update the Skuid Session Table\n    SkuidSessionVariablesModel = skuid.model.getModel('SkuidSessionVariables'),\n    SkuidSessionVariablesRow = SkuidSessionVariablesModel.getFirstRow();\n\n    SkuidSessionVariablesModel.updateRow(SkuidSessionVariablesRow,'Lender__c',nameField[0]);\n    // Make sure that the Account Field Editor is updated\n    $.each(SkuidSessionVariablesModel.registeredItems,function(){\n        this.refreshFields();\n    });\n    \n    \n    // Finally, navigate to step2    \n    step.navigate('step2');\n    \n    \n\n}\n\n\n\n\n\n\n</jsitem><jsitem location=\"inlinesnippet\" name=\"getLenderPlanId\" cachelocation=\"false\">var params = arguments[0],\n    step = params.step,\n$ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar CHECKBOX_FIELD = \"Select_Item__c\";\n\nvar idsOfRowsThatAreChecked = [];\nvar nameField = [];\n\n//Get Application Id\nvar ApplicationModel = skuid.model.getModel('Application');\nvar appId = ApplicationModel.data[0].Id;\n\n//Get Model\nvar QuoteWizardStep2Model = skuid.model.getModel('QuoteWizardStep2');\n\n    \n//Loop through all rows an