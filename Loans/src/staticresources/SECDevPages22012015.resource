tanding Items have been created')\n\n//Refresh Updated Models\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\nvar ApplicationModel = skuid.model.getModel('Application');\n\nOutstandingItemModel.updateData();\nApplicationModel.updateData();\nOutstandingItemNotReceivedModel.updateData();\n\n}\nelse{\nalert('There has been an error')\n}\n}\ncatch(err){\nalert(err);\n}</jsitem><jsitem location=\"external\" name=\"apex\" cachelocation=\"false\" url=\"/soap/ajax/29.0/apex.js\">var params = arguments[0],\n   $ = skuid.$;\n</jsitem><jsitem location=\"inlinesnippet\" name=\"ConfirmPackReceived\" cachelocation=\"false\" url=\"\">var params = arguments[0],\n    $ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar CHECKBOX_FIELD = \"Received__c\";\nvar idsOfRowsThatAreChecked = [];\n\n//Get Model\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\n\n    \n//Loop through all rows and grab the Id's of the ones checked as Received\n$.each(OutstandingItemModel.data,function(i,row){\n   if (OutstandingItemModel.getFieldValue(row,CHECKBOX_FIELD)===true){\n        idsOfRowsThatAreChecked.push(row.Id);\n   }\n});\n\nif (idsOfRowsThatAreChecked.length &gt; 0){\ntry{\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"AppSummary\",\"packReceivedButton\",{OutstandingItems:idsOfRowsThatAreChecked})); \n\nif(sResponse == \"complete\"){\n\nalert('Pack Received Confirmation')\n\n//Refresh Updated Models\nvar ApplicationModel = skuid.model.getModel('Application');\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\nskuid.model.cancel([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\nskuid.model.updateData([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\n\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\nelse{\nalert('There has been an error')\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\n}\ncatch(err){\nalert(err);\n}\n}\nelse{\nalert('Please select at least one Outstanding Item')\n\n}\n\n\n</jsitem><jsitem location=\"inlinesnippet\" name=\"ConfirmSentToBank\" cachelocation=\"false\" url=\"\">var params = arguments[0],\n    $ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar ApplicationModel = skuid.model.getModel('Application');\nvar appId = ApplicationModel.data[0].Id;\n\nvar CHECKBOX_FIELD = \"Received__c\";\nvar idsOfRowsThatAreChecked = [];\n\n//Get Model\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\n\nvar allItemsChecked = true;    \n//Loop through all rows and grab the Id's of the ones checked as Received\n$.each(OutstandingItemNotReceivedModel.data,function(i,row){\n   if (OutstandingItemNotReceivedModel.getFieldValue(row,CHECKBOX_FIELD)===false){\n        allItemsChecked = false;\n   }\n   else if (OutstandingItemNotReceivedModel.getFieldValue(row,CHECKBOX_FIELD)===true){\n        idsOfRowsThatAreChecked.push(row.Id);\n   }\n});\n\nif (allItemsChecked == true){\ntry{\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"AppSummary\",\"sentToBankButton\",{applicationId:appId, OutstandingItems:idsOfRowsThatAreChecked})); \n\nif(sResponse == \"complete\"){\n\nalert('Sent to Bank Confirmation')\n\n//Refresh Updated Models\nvar ApplicationModel = skuid.model.getModel('Application');\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\n\nskuid.model.cancel([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\nskuid.model.updateData([OutstandingItemModel,ApplicationModel,OutstandingItemNotReceivedModel]);\n\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\nelse{\nalert('There has been an error')\n//Close the Popup\nskuid.$('.ui-dialog-content').dialog('close');\n}\n}\ncatch(err){\nalert(err);\n}\n}\nelse{\nalert('All Outstanding Items need to be checked to Send to Bank')\n\n}\n\n\n</jsitem><jsitem location=\"inlinesnippet\" name=\"SyncModels\" cachelocation=\"false\">\nskuid.$(function(){\n\nvar ChildModel = skuid.model.getModel('OutstandingItemNotReceived'),\nParentModel = skuid.model.getModel('OutstandingItem');\n\nvar ChildModel2 = skuid.model.getModel('OutstandingItem'),\nParentModel2 = skuid.model.getModel('OutstandingItemNotReceived');\n\nvar listener = new skuid.ui.Editor();\nlistener.handleSave = function() {\nif (!ParentModel.hasChanged) {\nParentModel.updateData();\n}\nif (!ParentModel2.hasChanged) {\nParentModel2.updateData();\n}\n};\nlistener.registerModel(ChildModel);\nlistener.registerModel(ChildModel2);\n}); </jsitem><jsitem location=\"inlinesnippet\" name=\"PaidOut\" cachelocation=\"false\">var params = arguments[0],\n    $ = skuid.$;\n\n//Declare variable for the Field to retrieve values from    \nvar CHECKBOX_FIELD = \"Received__c\";\nvar idsOfRowsThatAreChecked = [];\n\n//Get Model\nvar OutstandingItemModel = skuid.model.getModel('OutstandingItem');\n\nvar paidOut = true;    \n//Loop through all rows and grab the Id's of the ones checked as Received\n$.each(OutstandingItemModel.data,function(i,row){\n   if (OutstandingItemModel.getFieldValue(row,CHECKBOX_FIELD)===false){\n        paidOut = false;\n   }\n   else{\n       idsOfRowsThatAreChecked.push(row.Id);\n   }\n});\n\nif (paidOut == true &amp;&amp; idsOfRowsThatAreChecked.length &gt; 0){\ntry{\n//Call the Apex Webservice Method\nvar sResponse = new String(sforce.apex.execute(\"AppSummary\",\"paidOut\",{OutstandingItems:idsOfRowsThatAreChecked})); \n\nif(sResponse == \"complete\"){\n\nalert('Paid Out Confirmation')\n\n//Refresh Updated Models\nvar ApplicationModel = skuid.model.getModel('Application');\nvar OutstandingItemNotReceivedModel = skuid.model.getModel('OutstandingItemNotReceived');\nskuid.model.cancel([OutstandingItemModel,ApplicationModel,Outsta