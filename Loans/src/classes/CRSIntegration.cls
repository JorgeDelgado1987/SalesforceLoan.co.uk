    public class AccountHistory implements Comparable{
        public String mField;
        public Integer balField;
        public Boolean balFieldSpecified;
        public Integer limitField;
        public Boolean limitFieldSpecified;
        public String accField;
        public String payField;
        public Integer stmtbalField;
        public Boolean stmtbalFieldSpecified;
        public Integer payamtField;
        public Boolean payamtFieldSpecified;
        public Integer cashadvcountField;
        public Boolean cashadvcountFieldSpecified;
        public Integer cashadvtotalField;
        public Boolean cashadvtotalFieldSpecified;

        public Integer compareTo(Object compareTo){
            AccountHistory history_to_compare = (AccountHistory)compareTo;
            if(getmFieldAsDate() == history_to_compare.getmFieldAsDate()) return 0;
            if(getmFieldAsDate() > history_to_compare.getmFieldAsDate()) return -1;
            else return 1;
        }

        private Date getmFieldAsDate(){
            String year = mField.substringBefore('-');
            String month = mField.substringAfter('-');
            return Date.newInstance(Integer.valueOf(year), Integer.valueOf(month), 1);
        }

        public String getPaymentCode(){
            Set<String> translate_to_0 = new Set<String>{'?','U','/','',null};
            if(payField.isNumeric()){
                return payField;
            } else if (translate_to_0.contains(payField)){
                return '0';
            } else {
                return payField;
            }
        }

    }

