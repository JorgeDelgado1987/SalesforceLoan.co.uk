    public static Report getCreditReport(Application__c application){
        if(String.isNotEmpty(application.CRS_Report_Id__c)){
            return CRSIntegration.getCreditReport(application.CRS_Report_Id__c);
        } else {
            List<Person> persons = new List<Person>();
            for(Applicant__c applicant:[SELECT id,Title__c, Email_Address__c,Date_of_Birth__c,Maiden_Name__c,First_name__c,
                                    Last_name__c, Postcode__c FROM Applicant__c WHERE Application__c=:application.Id]){

                Person p = new Person().setForename(applicant.First_name__c)
                                    .setSurname(applicant.Last_name__c)
                                    .setEmail(applicant.Email_Address__c)
                                    .setDOB(applicant.Date_of_Birth__c)
                                    .setTitle(applicant.Title__c)
                                    .setPostCode(applicant.Postcode__c);
                if(String.isNotEmpty(applicant.Maiden_Name__c))
                    p.setOtherNames(applicant.Maiden_Name__c);
                persons.add(p);
            }

            for(Property__c property:[SELECT id, House_Name_Number__c, Purchase_Date__c,Date_Moved_Out__c, Date_Moved_In__c,
                                        First_Line_of_Address__c, Town__c, County__c, Postcode__c, Street_Name__c
                                        FROM Property__c
                                        WHERE Application__c=:application.Id
                                        AND Current_Address__c =:true]){
                for(Person p:persons){
                    if(property.House_Name_Number__c != null){
                        if(property.House_Name_Number__c.isNumeric()){
                            p.setBuildingNo(property.House_Name_Number__c);
                        } else {
                            p.setBuildingName(property.House_Name_Number__c);
                        }
                    }
                    p.setStreet1(property.Street_Name__c)
                        .setTown(property.Town__c)
                        .setPostCode(property.Postcode__c);
                }

            }
            CreditSearchRequest search_request = new CreditSearchRequest();
            search_request.ReferenceId = application.Id;
            search_request.SearchParams = persons;
            return CRSIntegration.getCreditReport(search_request);
        }
    }

