        return JSON.serialize(search_request);
    }

    @TestVisible
    private void validateInputModel(CreditSearchRequest search_request){
        if(search_request.SearchParams == null || search_request.SearchParams.isEmpty()){
            throw new ValidationException('No Search Parameters found, at least one persons details must be supplied');
        }
        for(Person p:search_request.SearchParams){
            if(p.Forename == null || String.isBlank(p.Forename)){
                throw new ValidationException('Forename can not be blank');
            }
            if(p.Forename.length() > 30){
                throw new ValidationException('Forename can not be longer than 30 characters');
            }
            if(p.Surname == null || String.isBlank(p.Surname)){
                throw new ValidationException('Surname can not be blank');
            }
            if(p.Surname.length() > 30){
                throw new ValidationException('Surname can not be longer than 30 characters');
            }
            if(String.isNotBlank(p.Title) && p.Title.length() > 30){
                throw new ValidationException('Title can not be longer than 30 characters');
            }
            if(String.isNotBlank(p.OtherNames) && p.OtherNames.length() > 30){
                throw new ValidationException('Other Names can not be longer than 30 characters');
            }
            if(String.isNotBlank(p.PostCode) && p.PostCode.length() > 8){
                throw new ValidationException('Post Code can not be longer than 8 characters');
            }
            if(String.isNotBlank(p.Town) && p.Town.length() > 25){
                throw new ValidationException('Town can not be longer than 25 characters');
            }
            if(String.isNotBlank(p.BuildingNo) && p.BuildingNo.length() > 12){
                throw new ValidationException('Building Number can not be longer than 12 characters');
            }
            if(String.isNotBlank(p.BuildingName) && p.BuildingName.length() > 50){
                throw new ValidationException('Building Name can not be longer than 50 characters');
            }
            if(String.isNotBlank(p.Street1) && p.Street1.length() > 50){
                throw new ValidationException('Street Line 1 can not be longer than 50 characters');
            }
            if(String.isNotBlank(p.Street2) && p.Street2.length() > 50){
                throw new ValidationException('Street Line 2 can not be longer than 50 characters');
            }
            if(String.isBlank(p.Street1) || String.isBlank(p.PostCode)){
                throw new ValidationException('A valid address is required, a First Line and Post Code must be supplied');
            }
        }
    }

