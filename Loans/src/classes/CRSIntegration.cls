    private HttpRequest buildHttpRequest(CreditSearchRequest search_request){
        HttpRequest http_request = new HttpRequest();
        http_request.setEndpoint(getCRSEndpoint());
        http_request.setMethod('POST');
        http_request.setTimeout(HTTP_TIMEOUT);
        http_request.setHeader('Content-Type','application/json');
        http_request.setHeader('Authorization-Token', AUTHORIZATION);
        http_request.setBody(getJSON(search_request));
        return http_request;
    }

    private String getCRSEndpoint(){
        String url_suffix = '/api/CreditSearchSf';
        if(Utility.isRunningInSandbox()){
            return 'http://0f804cbb17c3455e98aa2c9e668faa71.cloudapp.net' + url_suffix;
        } else {
            return 'https://crservice.sterlingventures.net' + url_suffix;
        }
    }

