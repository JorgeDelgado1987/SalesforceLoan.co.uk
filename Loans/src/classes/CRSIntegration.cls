        }

        public Boolean isMortgage(){
            return this.accdetailsField.isMortgage();
        }

        public Boolean settledInLastMonths(Integer number_of_months){
            return isSettled() && ((accdetailsField.arrenddatefieldspecified && accdetailsField.arrenddateField > DateTime.now().addMonths(-number_of_months)) ||
                (accdetailsField.accenddatefieldspecified && accdetailsField.accenddateField > DateTime.now().addMonths(-number_of_months)));
        }

        public Boolean isSettled(){
            return this.accdetailsField.statusField == 'S';
        }

        public Boolean isNotSettled(){
            return !isSettled();
        }

        public String getRepaymentProfile(){
            return getRepaymentProfile(128);
        }

        public String getRepaymentProfile(Integer number_of_months){
            String payment_history_string = '';
            acchistoryField.sort();
            Iterator<AccountHistory> history_iterator = acchistoryField.iterator();
            AccountHistory current_history;
            while (history_iterator.hasNext()) {
                current_history = history_iterator.next();
                payment_history_string += current_history.getPaymentCode();
            }
            if(number_of_months > payment_history_string.length()){
                return payment_history_string;
            } else {
                return payment_history_string.substring(0,number_of_months);
            }
        }

        public Integer getArrearsInMonths(Integer number_of_months){
            Integer arrears_count = 0;
            List<String> history_chars = getRepaymentProfile(number_of_months).reverse().split('');
            history_chars.remove(0);
            Iterator<String> arrears_string = history_chars.iterator();
            String current_string;
            String last_string;
            while(arrears_string.hasNext()){
                if(current_string == null){
                    last_string = arrears_string.next();
                    current_string = last_string;
                } else {
                    last_string = current_string;
                    current_string = arrears_string.next();
                }
                if(last_string != current_string){
                    if(getIntegerForArrearsString(current_string) > getIntegerForArrearsString(last_string)){
                        arrears_count++;
                    }
                }
            }
            return arrears_count;
        }

        private Integer getIntegerForArrearsString(String arrear_string){
            Integer current_value = 0;
            if(arrear_string != null){
                if(arrear_string.isNumeric()){
                    current_value = Integer.valueOf(arrear_string);
                } else if(arrear_string == 'D'){
                    current_value = 10;
                }
            }
            return current_value;
        }

        public Decimal monthlyRepaymentAmount(){
            if(getStatus() == 'Settled' || getStatus() == 'Satisfied' || accdetailsField.balanceField == 0){
                return 0;
            }else if(accdetailsField.regpaymentFieldSpecified){
                return accdetailsField.regpaymentField;
            } else if(acchistoryField.size() == 1){
                return acchistoryField.get(0).balField;
            } else {
                Decimal monthly_repayment = 0;
                acchistoryField.sort();
                System.debug(acchistoryField);
                List<Integer> all_diffs = new List<Integer>();
                for(Integer i = 0; i < acchistoryField.size()-1; i++){
                    Integer diff = acchistoryField.get(i+1).balField - acchistoryField.get(i).balField;
                    all_diffs.add(Math.abs(diff));
                }
                System.debug(all_diffs);
                Integer total = 0;
                for(Integer i:all_diffs){
                    total += i;
                }
                system.debug(total);
                monthly_repayment = total/acchistoryField.size();
                return monthly_repayment;
            }
        }
    }

